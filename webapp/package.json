{
  "name": "@mattermost/webapp",
  "private": true,
  "engines": {
    "node": ">=24.11.0",
    "npm": "^11.0.0"
  },
  "scripts": {
    "postinstall": "patch-package && npm run build --workspace=platform/types --workspace=platform/client --workspace=platform/components",
    "build": "node scripts/build-vite.mjs",
    "run": "npm run run --workspace=channels",
    "dev-server": "npm run dev-server --workspace=channels",
    "dev": "npm run dev --workspace=channels",
    "test": "npm run test --workspaces --if-present",
    "test:updatesnapshot": "npm run test:updatesnapshot --workspaces --if-present",
    "test-ci": "npm run test-ci --workspaces --if-present",
    "check": "npm run check --workspaces --if-present",
    "fix": "npm run fix --workspaces --if-present",
    "check-types": "npm run check-types --workspaces --if-present",
    "clean": "npm run clean --workspaces --if-present",
    "gen-lang-imports": "node scripts/gen_lang_imports.mjs"
  },
  "dependencies": {
    "@mattermost/compass-icons": "0.1.52",
    "typescript": "5.6.3"
  },
  "devDependencies": {
    "@types/node": "24.10.1",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitejs/plugin-react-swc": "4.2.2",
    "blessed": "0.1.81",
    "chalk": "5.3.0",
    "concurrently": "9.0.1",
    "cross-env": "7.0.3",
    "eslint": "8.57.0",
    "eslint-import-resolver-vite": "2.1.0",
    "eslint-plugin-formatjs": "4.12.2",
    "eslint-plugin-react": "7.34.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "patch-package": "8.0.0",
    "rollup-plugin-visualizer": "6.0.5",
    "sass": "1.80.5",
    "strip-ansi": "7.1.0",
    "typescript-eslint-language-service": "5.0.5",
    "vite": "8.0.0-beta.0",
    "vite-plugin-checker": "0.11.0"
  },
  "overrides": {
    "@mui/system": "5.11.16",
    "@mui/styled-engine": "5.11.16",
    "@deanwhillier/jest-matchmedia-mock": {
      "jest": "30.1.3"
    },
    "@giphy/react-components": {
      "styled-components": "5.3.7"
    },
    "@types/redux-mock-store": {
      "redux": "5.0.1"
    },
    "braces": "3.0.3",
    "cheerio": "1.0.0-rc.12",
    "@testing-library/dom": {
      "dom-accessibility-api": "0.6.3"
    },
    "@testing-library/jest-dom": {
      "dom-accessibility-api": "0.6.3"
    },
    "@types/estree": "0.0.51",
    "asn1.js": "5.4.1",
    "react-beautiful-dnd": {
      "react-redux": "9.2.0"
    },
    "redux-persist": {
      "redux": "5.0.1"
    }
  },
  "workspaces": [
    "channels",
    "platform/client",
    "platform/components",
    "platform/eslint-plugin",
    "platform/mattermost-redux",
    "platform/types"
  ]
}
